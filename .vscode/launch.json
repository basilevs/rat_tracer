    "version": "0.2.0",
    "configurations": [



        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "copy_static_label",
            "type": "debugpy",
            "request": "launch",
            "program": "rat_tracer/spread_static_label.py",
            "console": "integratedTerminal",
            "args": "data/labels/Train"
        },
        {
            "name": "find_worst_annotations",
            "type": "debugpy",
            "request": "launch",
            "program": "rat_tracer/find_worst_annotations.py",
            "console": "integratedTerminal",
            "args": ""
        },
        {
            "name": "find_moves",
            "type": "debugpy",
            "request": "launch",
            "program": "rat_tracer/find_moves.py",
            "console": "integratedTerminal",
            "args": "2 data/labels/Train"
        },
        {
            "name": "paint",
            "type": "debugpy",
            "request": "launch",
            "program": "rat_tracer/paint.py",
            "console": "integratedTerminal",
            "args": "input/2026-02-07-2.mp4 /tmp/"
        }
        {
            "name": "track",
            "type": "debugpy",
            "request": "launch",
            "program": "rat_tracer/track.py",
            "console": "integratedTerminal",
            "args": "input/2026-02-07-1.mp4"
        }
    ]
}