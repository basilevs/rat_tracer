{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "copy_static_label",
            "type": "debugpy",
            "request": "launch",
            "program": "rat_tracer/spread_static_label.py",
            "console": "integratedTerminal",
            "args": "data/labels/Train"
        },
        {
            "name": "find_moves",
            "type": "debugpy",
            "request": "launch",
            "program": "rat_tracer/find_moves.py",
            "console": "integratedTerminal",
            "args": "2 data/labels/Train"
        }
    ]
}